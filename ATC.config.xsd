<?xml version="1.0" encoding="utf-8"?>
<xs:schema id="ATCTroops" xmlns="" xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:msdata="urn:schemas-microsoft-com:xml-msdata">
  <xs:element name="ATCTroops" msdata:IsDataSet="true" msdata:Locale="en-US">
    <xs:complexType>
      <xs:choice minOccurs="0" maxOccurs="unbounded">
        <xs:element name="debugInfo">
          <xs:complexType>
            <xs:sequence>
              <xs:element name="troop_replacement" type="xs:string" minOccurs="0" />
              <xs:element name="show_percentage" type="xs:string" minOccurs="0" />
              <xs:element name="playeronly_flag" type="xs:string" minOccurs="0" />
            </xs:sequence>
          </xs:complexType>
        </xs:element>
        <xs:element name="source_troop">
          <xs:complexType>
            <xs:sequence>
              <xs:element name="target_troop" minOccurs="0" maxOccurs="unbounded">
                <xs:complexType>
                  <xs:attribute name="id" type="xs:string" />
                  <xs:attribute name="percent" type="xs:string" />
                  <xs:attribute name="playeronly" type="xs:string" />
                </xs:complexType>
              </xs:element>
            </xs:sequence>
            <xs:attribute name="id" type="xs:string" />
          </xs:complexType>
        </xs:element>
      </xs:choice>
    </xs:complexType>
  </xs:element>
</xs:schema>