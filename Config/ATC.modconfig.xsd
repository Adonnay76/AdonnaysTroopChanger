													</xs:sequence>
												</xs:complexType>
											</xs:element>
										</xs:sequence>
										<xs:attribute name="id" type="xs:string" use="required" />
									</xs:complexType>
								</xs:element>
							</xs:sequence>
							<xs:attribute name="id" type="xs:string" use="required" />
						</xs:complexType>
					</xs:element>
					<xs:element name="mapfaction">
						<xs:complexType>
							<xs:sequence>
								<xs:element name="culture">
									<xs:complexType>
										<xs:sequence>
											<xs:element name="basictroops">
